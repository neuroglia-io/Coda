<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Charles d'Avernas</Authors>
    <Company>Neuroglia SPRL</Company>
    <Description>.NET Standard 2.1 library for parsing Belgian CODA files</Description>
    <Copyright>Copyright © 2020 Neuroglia SPRL. All rights reserved.</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/neuroglia-io/Data.Coda</PackageProjectUrl>
    <RepositoryUrl>https://github.com/neuroglia-io/Data.Coda</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>neuroglia data coda</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageIcon>neurologo.jpg</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Users\charl\Pictures\neurologo.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
